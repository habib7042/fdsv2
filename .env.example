# Database Configuration
# SQLite Database URL (for local development)
DATABASE_URL=file:./dev.db

# MongoDB Database URI (for production or alternative database)
# Replace with your actual MongoDB connection string
MONGODB_URI=mongodb://localhost:27017/fds-database

# Alternatively, use MongoDB Atlas (cloud database)
# MONGODB_URI=mongodb+srv://username:password@cluster0.mongodb.net/fds-database?retryWrites=true&w=majority

# Next.js Configuration
NEXTAUTH_SECRET=your-nextauth-secret-key-here
NEXTAUTH_URL=http://localhost:3000

# Optional: Environment Configuration
NODE_ENV=development

# Optional: Database Seed Configuration
# Set to "true" to run database seeding on startup
RUN_SEED=false

# Optional: API Configuration
# Add any third-party API keys here
# API_KEY=your-api-key-here